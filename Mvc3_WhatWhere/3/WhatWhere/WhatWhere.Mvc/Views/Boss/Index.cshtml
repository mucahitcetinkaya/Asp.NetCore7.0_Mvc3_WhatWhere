@model List<Boss>


<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">FirstName</th>
            <th scope="col">LastName</th>
            <th scope="col">Gender</th>
            <th scope="col">BirthOfYear</th>
            <th scope="col">PhoneNumber</th>
            <th scope="col">MailAdress</th>
        </tr>
    </thead>

    @foreach (var boss in Model)
    {
        <tbody>
            <tr>
                <th>@boss.Id</th>
                <td>@boss.FirstName</td>
                <td>@boss.LastName</td>
                <td>@boss.Gender</td>
                <td>@boss.BirthOfYear</td>
                <td>@boss.PhoneNumber</td>
                <td>@boss.MailAdress</td>
            </tr>
        </tbody>
    }
</table>