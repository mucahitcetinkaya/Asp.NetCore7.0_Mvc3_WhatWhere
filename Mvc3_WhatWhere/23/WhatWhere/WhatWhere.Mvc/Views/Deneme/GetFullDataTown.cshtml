@model List<Town>


<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Adı</th>
            <th scope="col">CityId</th>
            <th scope="col">City Name</th>
        </tr>
    </thead>

    @foreach (var town in Model)
    {
        <tbody>
            <tr>
                <th>@town.Id</th>
                <td>@town.Ilce</td>
                <td>@town.CityId</td>
                <td>@town.City.Sehir</td>
            </tr>
        </tbody>
    }
</table>
