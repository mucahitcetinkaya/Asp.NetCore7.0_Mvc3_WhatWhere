@model List<TownViewModel>

<h1>GetFullDataTown1</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Adı</th>
      <th scope="col">Sehir Plaka</th>
      <th scope="col">Sehir Adı</th>
    </tr>
  </thead>

  <tbody>
        @foreach (var town in Model)
        {
        <tr>
          <td>@town.Id</td>
          <td>@town.Ilce</td>
          <td>@town.CityId</td>
          <td>@town.City.Sehir</td>
        </tr>
        }
  </tbody>
</table>

